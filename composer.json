{
  "name": "apiato/apiato",
  "description": "A flawless framework for building scalable and testable API-Centric Apps with PHP and Laravel.",
  "homepage": "http://apiato.io/",
  "support": {
    "issues": "https://github.com/apiato/apiato/issues",
    "source": "https://github.com/apiato/apiato"
  },
  "authors": [
    {
      "name": "Mahmoud Zalt",
      "email": "mahmoud@zalt.me"
    }
  ],
  "keywords": [
    "Apiato",
    "API",
    "Framework",
    "API framework",
    "API Starter",
    "Laravel API",
    "PHP",
    "Hello API",
    "Porto",
    "Porto SAP"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-tokenizer": "*",
    "barryvdh/laravel-snappy": "^0.4.1",
    "enniel/laravel-fcm-notification-channel": "1.*",
    "cyrildewit/eloquent-viewable": "^1.0",
    "doctrine/dbal": "2.5.*",
    "faustbrian/laravel-commentable": "^3.1",
    "fideloper/proxy": "~3.3",
    "firebase/php-jwt": "~4.0",
    "frozennode/administrator": " 5.*",
    "h4cc/wkhtmltoimage-amd64": "0.12.x",
    "h4cc/wkhtmltopdf-amd64": "0.12.x",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "^1.0",
    "league/glide": "^1.2",
    "orangehill/iseed": "dev-master",
    "overtrue/laravel-follow": "^1.1",
    "rtconner/laravel-tagging": "~2.2",
    "spatie/laravel-medialibrary": "^6.0.0",
    "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
    "wikimedia/composer-merge-plugin": "^1.3.1"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "^1.7",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "deployer/deployer": "^6.0",
    "laravel/homestead": "^5.2"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {

    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "*"
      ]
    },
    "merge-plugin": {
      "include": [
        "app/Ship/composer.json",
        "app/Containers/*/composer.json",
        "app/Containers/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": true,
      "merge-extra": false,
      "merge-extra-deep": false,
      "merge-scripts": true
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,

  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  }
}
